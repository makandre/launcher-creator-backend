    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "nodejs health",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "debug-apply",
                "/opt/Repos/makandre/test_nodejs",
                "--runtime",
                "nodejs",
                "test_nodejs",
                "health"
